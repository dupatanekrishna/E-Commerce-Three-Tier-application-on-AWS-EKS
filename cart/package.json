{
  "name": "cart",
  "version": "1.0.0",
  "description": "cart REST API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest --coverage --coverageDirectory=coverage --coverageReporters=lcov",
    "test:watch": "jest --watch"
  },
  "author": "SteveW",
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "^1.18.1",
    "express": "^4.15.4",
    "redis": "^2.8.0",
    "request": "^2.88.2",
    "pino": "^5.10.8",
    "express-pino-logger": "^4.0.0",
    "pino-pretty": "^2.5.0",
    "@instana/collector": "^1.132.2",
    "prom-client": "^11.5.3"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "supertest": "^6.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
